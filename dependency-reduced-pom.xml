<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.xiaqing</groupId>
  <artifactId>javaweb</artifactId>
  <version>1.0</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.2.4</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
        <configuration />
      </plugin>
    </plugins>
  </build>
  <properties>
    <jakarta.inject-api.version>2.0.1</jakarta.inject-api.version>
    <maven.compiler.target>1.8</maven.compiler.target>
    <mssql-jdbc.version>9.4.1.jre16</mssql-jdbc.version>
    <maven.compiler.source>1.8</maven.compiler.source>
    <jakarta.servlet.jsp.jstl.version>2.0.0</jakarta.servlet.jsp.jstl.version>
    <commons-lang3.version>3.12.0</commons-lang3.version>
    <jakarta.jakartaee-api.version>9.0.0</jakarta.jakartaee-api.version>
    <weld.servlet.version>4.0.2.Final</weld.servlet.version>
    <jackson.version>2.13.1</jackson.version>
    <commons-beanutils.version>1.9.4</commons-beanutils.version>
  </properties>
</project>
